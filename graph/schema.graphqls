type Query {
  helloWorld: String
}

type Mutation {
  createUser(input: CreateUser!): User
}

type User {
  id: ID!
  name: String!
  provider: String
  email: String
  avi: String
  sex: String
  username: String
  jwt: String
}

input CreateUser {
  token: String!
  email: String!
  name: String!
  provider: String!
  avi: String
  sex: String
  username: String
}

